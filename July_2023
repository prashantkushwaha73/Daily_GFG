****************************************************************************************
**** Number of 1 Bits ( 01/07/23 ) ****

 class Solution {
  public:
    int setBits(int N) {
        int ans = 0;
        while(N){
            if(N%2){
                ans++;
                N/=2;
            }
            else{
                N/=2;
            }
        }
        return ans;
    }
};

******************************************************************************

